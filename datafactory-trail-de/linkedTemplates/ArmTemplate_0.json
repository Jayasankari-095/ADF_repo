				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": {
						"value": "@dataset().rest_api_schema",
						"type": "Expression"
					},
					"table": {
						"value": "@dataset().rest_api_table",
						"type": "Expression"
					}
				}
			},
